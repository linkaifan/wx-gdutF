.weui-search-bar{
    padding: 8rpx;
}
.weui-search-bar__cancel-btn{
    margin-left: 8rpx;
}
.searchList{
    width: 750rpx;
    height: 100%;
    font-size: 30rpx;
    color: #353535;
    align-items: flex-start;
    justify-content: flex-start;
    flex-direction: column;
    text-align: center;
    margin-top: 25rpx;

}
#input{
	background:#fff;
}
.sousuo{
  font-size: 32rpx;
  white-space:nowarp;
    -webkit-flex: 1;
          flex: 1;
  color: #41ae8e;
}
.keyword_item{
	display:block;
	float:left;
	margin-left:12px;
	border:1px solid lightgray;
	background:white;
	padding:4px 6px;
	margin-bottom:12px;
	border-radius:2px;
  font-size: 17px;
}
.searchItem{
  font-size: 16px;
    width: 100rpx;
    height: 100rpx;
    overflow: hidden;
    padding: 10rpx 0 0 0;
}
.undefined{
  display: none;
  margin: 20rpx auto;
  justify-content: flex-start;
  align-items: center;
  height:95rpx;
  width: 700rpx;
  z-index: 10;
}
.bookList{
    width: 700rpx;
    height: 180rpx;
    margin: 20rpx auto;
    border: 1px solid #FAFAFA;
    border-radius: 20rpx;
    background: #FFFFFF;
    display: flex;
    justify-content: flex-start;
    align-items: center;
}
.bookLogo{   
    width: 108rpx;
    height: 90rpx;
    margin: 0 20rpx 0 30rpx;
}
.bookInfo{
    width: 500rpx;
    font-size: 25rpx;
    color: #888888; 
    display: block;    
}
.title{
  display: block;
    /*height: 65rpx;*/
    line-height: 45rpx;
    font-size: 35rpx;
    color: #353535;
    overflow: hidden; 
    padding-right:80rpx;
    padding-left: 20rpx
}
.author{
  font-size: 28rpx;
    height: 43rpx;
    line-height: 38rpx;
    overflow: hidden; 
    padding-right:80rpx;
    padding-left: 20rpx
}
.weui-cell__ft_in-access::after{
    width: 20rpx;
    height: 20rpx;
    border-color: #41ae8e;
    position: relative;
    right: 10rpx;
}